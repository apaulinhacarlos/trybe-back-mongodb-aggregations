use("aggregations");
db.movies.find();

use("aggregations");
db.movies.aggregate([
  { 
    $match: {
      $and: [
        { "imdb.rating": { $gte: 7 } },
        { genres: { $nin: ["Crime", "Horror"] } },
        { rated: { $in: ["PG", "P"] } },
        { languages: { $in: ["English", "Spanish"] } },


      ]

    }
  },
]).itcount();